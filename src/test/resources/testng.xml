<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="Listener.CustomListener"></listener>
	</listeners>
	<test thread-count="3" name="ChromeTest">
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="Smoke"></include>
				<include name="Regression"></include>
			</run>
		</groups>
		<classes>
			<class name="com.evisa.qa.testcases.LoginPageTest" />
		</classes>
	</test>

	<test thread-count="3" name="ChromeTest2" >
		<parameter name="browser" value="chrome"></parameter>
		<groups>
			<run>
				<include name="Smoke"></include>
				<include name="Regression"></include>
			</run>
		</groups>
		<classes>
			<class name="com.evisa.qa.testcases.ApplyVisaTest"/>
		</classes>
	</test>

</suite> 